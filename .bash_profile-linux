export PS1=":\w\$ "
export PATH="$PATH:$HOME/bin:$HOME/go/bin:/usr/local/go/bin:$HOME/.cargo/bin"
export EDITOR="vim"
export GPG_TTY=$(tty)
export HISTTIMEFORMAT="%s "
PROMPT_COMMAND="${PROMPT_COMMAND:+$PROMPT_COMMAND ; }"'echo $$ $USER "$(history 1)" >> ~/dropbox/apps/bash/bash-eternal-history'
export CHOKIDAR_USEPOLLING=1
export ENABLE_FORK_TS_CHECKER=false
export DOOER_SERVICE_SKIP_VALIDATION=true

alias ls="ls -F"
alias tf_plan="./terraform.sh plan -lock-timeout=5m -out terraform.plan"
alias tf_apply="./terraform.sh apply terraform.plan"
alias vim="vim -p"
alias er="environment-remote -u oscar.linderholm -e osm"
alias jq="jq -M"
alias dumpinen="dumpinen | xargs echo $(date "+%Y-%m-%d %H:%M:%S") | tee -a $HOME/.dumpinen"
alias feh="feh -F"

function lcd {
	cd $(tail -n 1000 "$HOME/dropbox/apps/bash/bash-eternal-history" | grep "cd " | tail -n 1 | cut -d' ' -f9-)
}
